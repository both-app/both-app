{
  "expo": {
    "name": "Both",
    "slug": "Both",
    "privacy": "public",
    "platforms": [
      "ios"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#F9F0EB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "notification": {
      "iosDisplayInForeground": true,
      "color": "#0A1D2B"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "description": "Share love & daily tasks",
    "githubUrl": "https://github.com/mathieuletyrant/both-app",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "both",
            "project": "both-app",
            "authToken": "d639bbc54bed494db9425fddad35b83ca2fb55ba1f6e4525b834f66ec5475fb4",
            "url": ""
          }
        },
        {
          "file": "expo-postpublish-slack-notify",
          "config": {
            "webhookUrl": "https://hooks.slack.com/services/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",
            "channel": "#new-release"
          }
        }
      ]
    }
  }
}
